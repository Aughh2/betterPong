config_version=5

[application]

config/name="betterPong"
config/features=PackedStringArray("4.4", "Mobile")
config/icon="res://icon.svg"

[autoload]

Log="*res://addons/GoLogger/Log.tscn"
GameManager="*res://Scenes/game_manager.tscn"
EventBus="*res://Scripts/Event Bus/event_bus.gd"
UiController="*res://Scripts/UI Management/ui_controller.gd"
GameDataManager="*res://Scripts/Data Containers/game_data.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/GoLogger/plugin.cfg")

[file_customization]

folder_colors={
"res://Scenes/": "green",
"res://Scripts/": "red",
"res://Sprites/": "blue"
}

